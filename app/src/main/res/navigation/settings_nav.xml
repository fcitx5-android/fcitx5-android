<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/settings_nav"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="me.rocka.fcitx5test.ui.main.MainFragment"
        tools:layout="@layout/fragment_main">
        <action
            android:id="@+id/action_mainFragment_to_globalConfigFragment"
            app:destination="@id/globalConfigFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_mainFragment_to_addonListFragment"
            app:destination="@id/addonListFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_mainFragment_to_imListFragment"
            app:destination="@id/imListFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_mainFragment_to_behaviorSettingsFragment"
            app:destination="@id/behaviorSettingsFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_mainFragment_to_pinyinDictionaryFragment"
            app:destination="@id/pinyinDictionaryFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_mainFragment_to_developerFragment"
            app:destination="@id/developerFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_mainFragment_to_aboutFragment"
            app:destination="@id/aboutFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>

    <fragment
        android:id="@+id/globalConfigFragment"
        android:name="me.rocka.fcitx5test.ui.main.settings.global.GlobalConfigFragment" />

    <fragment
        android:id="@+id/imListFragment"
        android:name="me.rocka.fcitx5test.ui.main.settings.im.InputMethodListFragment">
        <action
            android:id="@+id/action_imListFragment_to_imConfigFragment"
            app:destination="@id/imConfigFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/imConfigFragment"
        android:name="me.rocka.fcitx5test.ui.main.settings.im.InputMethodConfigFragment">
        <action
            android:id="@+id/action_imConfigFragment_to_listFragment"
            app:destination="@id/listFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_imConfigFragment_to_pinyinDictionaryFragment"
            app:destination="@id/pinyinDictionaryFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>

    <fragment
        android:id="@+id/addonListFragment"
        android:name="me.rocka.fcitx5test.ui.main.settings.addon.AddonListFragment">
        <action
            android:id="@+id/action_addonListFragment_to_addonConfigFragment"
            app:destination="@id/addonConfigFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/addonConfigFragment"
        android:name="me.rocka.fcitx5test.ui.main.settings.addon.AddonConfigFragment">
        <action
            android:id="@+id/action_addonConfigFragment_to_listFragment"
            app:destination="@id/listFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_addonConfigFragment_to_pinyinDictionaryFragment"
            app:destination="@id/pinyinDictionaryFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>

    <fragment
        android:id="@+id/behaviorSettingsFragment"
        android:name="me.rocka.fcitx5test.ui.main.settings.behavior.BehaviorSettingsFragment" />

    <fragment
        android:id="@+id/listFragment"
        android:name="me.rocka.fcitx5test.ui.main.settings.ListFragment" />
    <fragment
        android:id="@+id/pinyinDictionaryFragment"
        android:name="me.rocka.fcitx5test.ui.main.settings.PinyinDictionaryFragment" />
    <fragment
        android:id="@+id/developerFragment"
        android:name="me.rocka.fcitx5test.ui.main.DeveloperFragment"
        android:label="DeveloperFragment">
        <action
            android:id="@+id/action_developerFragment_to_logFragment"
            app:destination="@id/logFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/aboutFragment"
        android:name="me.rocka.fcitx5test.ui.main.AboutFragment"
        android:label="AboutFragment" />
    <fragment
        android:id="@+id/logFragment"
        android:name="me.rocka.fcitx5test.ui.main.LogFragment"
        android:label="LogFragment" />

</navigation>